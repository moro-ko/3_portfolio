<div>検索枠</div>

<h4>プロジェクト一覧
  <!--<span>(全<%#= @projects.count %>件)</span>-->
</h4>
<% @projects.each do |project| %>
  <% if project.posting_status == "completed" %>
    <%= link_to project_path(project) do %>
      <%= image_tag project.image %>
      <%= project.title %>
      <p>現在支援金額</p>
      <p>支援者数</p>
      <p>残り</p>
    <% end %>
  <% end %>
<% end %>